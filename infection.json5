{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "source": {
        "directories": [
            "src"
        ]
    },
    "timeout": 30,
    "logs": {
        "text": "infection.log"
    },
    "mutators": {
        "@default": true,
         "@function_signature": false,
         "TrueValue": {
             "ignore": [
                 "NameSpace\\*\\Class::method"
             ],
         }
    }
}
